{
	"compilerOptions": {
	  "target": "es5",
	  "module": "commonjs",
	  "outDir": "out",
	  "rootDir": "src",
	  "typeRoots": ["./node_modules/byots/lib", "./node_modules/@types"],

	  "strict": true,
	  "lib": ["ES2015"],
	  "declaration": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"exclude": [
		"example/**/*",
		"out"
	]
}
